public class reverseNumber {
    public static void main (String args[]){
        int num = 12345;
        
        int reverse = 0;
        while(num > 0){
            int n = num%10;
            reverse = reverse*10 + n; // dont write-> reverse *= 10 + n;
            num /= 10;
        }
        System.out.print(reverse);
    }
}
