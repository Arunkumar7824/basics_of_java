public class leapYear {
    public static void main (String args[]){
        int year = 1988;
        if((year%4 == 0 && year%100 != 0) || (year%400 == 0)){
           System.out.print("The "+year+" is a leap year.");
        }
        else{
            System.out.print("The "+year+" is not a leap year.");
        }
    }
}
